      <div class="form-group">
        <%= f.label :name %>
        <%= f.text_field :name, class: "form-control", placeholder: "Name"%>
      </div>
      <div class="form-group">
        <%= f.label :company %>
        <%= f.text_field :company, class: "form-control", placeholder: "Company"%>
      </div>
      <div class="form-group">
        <%= f.label :email %>
        <%= f.text_field :email, class: "form-control", placeholder: "Email"%>
      </div>
      <div class="form-group">
        <%= f.label :phone, "Phone Number" %>
        <%= f.text_field :phone, class: "form-control", placeholder: "phone"%>
      </div> 
      <div class="form-group">
        <%= f.label :address %>
        <%= f.text_area :address, class: "form-control", placeholder: "Address"%>
      </div>
      <div class="form-group">
        <%= f.label :group %>
        <%= f.select(:group_id, @groups.map { |g| [g.name, g.id] }) %>
      </div>      
      <div class="form-group">
        <%= f.label :image, "IMage Upload" %>
        <%= f.file_field :image, class: "form-control", placeholder: "Upload your IMage"%>
      </div>      